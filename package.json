{
  "name": "@cobaltio/cobalt-js",
  "version": "8.9.2-beta.2",
  "description": "Cobalt frontend SDK",
  "main": "./cobalt.js",
  "typings": "./cobalt.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "npx typedoc cobalt.ts",
    "docs:llms": "npx typedoc cobalt.ts --plugin typedoc-plugin-markdown --mergeReadme --enumMembersFormat table --parametersFormat table --propertiesFormat table --typeDeclarationFormat table --indexFormat table --outputFileStrategy modules --cleanOutputDir false --fileExtension .txt --out docs --entryFileName llms",
    "test": "echo \"Warn: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gocobalt/cobalt-js.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "cobalt embed",
    "cobalt sdk",
    "frontend sdk",
    "sdk"
  ],
  "author": "Sankarsan Kampa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gocobalt/cobalt-js/issues"
  },
  "homepage": "https://github.com/gocobalt/cobalt-js#readme",
  "devDependencies": {
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "^5.3.2"
  }
}
